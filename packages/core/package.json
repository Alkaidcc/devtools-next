{
  "name": "@vue-devtools-next/core",
  "type": "module",
  "version": "0.0.1",
  "author": "webfansplz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/vue-devtools-next.git",
    "directory": "packages/core"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "types",
    "types.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "stub": "tsup --watch --onSuccess 'tsup --dts-only'",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@vue-devtools-next/app-core": "workspace:^",
    "@vue-devtools-next/shared": "workspace:^"
  }
}
